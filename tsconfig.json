{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2022",
    "noImplicitAny": true,
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "#/*": ["./src/*"],
    "emitDecoratorMetadata": ["true"],
    "experimentalDecorators": ["true"],
    }
  }
}
